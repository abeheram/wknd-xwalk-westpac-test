          {
            "name": "Card with Inline Image",
            "value": "inlineImage"
          },
          {
            "name": "Card with Tilted Image",
            "value": "tiltedImage"
          },
          {
            "name": "Card with Big Numbers",
            "value": "bignumber"
          },
          {
            "name": "Card with background image",
            "value": "background-image"
          }
