  {
    "id": "promotion",
    "fields": [
      {
        "component": "select",
        "label": "Type",
        "name": "type",
        "valueType": "string",
        "options": [
          {
            "name": "Promotion Card",
            "value": "promotion"
          },
          {
            "name": "In Context Card",
            "value": "incontext"
          },
          {
            "name": "Slim Banner",
            "value": "slim-banner"
          }
        ]
      },
      {
        "component": "select",
        "label": "Style",
        "name": "style",
        "valueType": "string",
        "condition": {
          "or": [
            {
              "===": [
                {
                  "var": "type"
                },
                "promotion"
              ]
            },
            {
              "==": [
                {
                  "var": "type"
                },
                "slim-banner"
              ]
            }
          ]
        },
        "options": [
          {
            "name": "Default",
            "value": "default"
